{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["jest", "node"] // Add "jest" here
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
